{
    "extensions": [
        "apcu"
    ],
    "ini": [
        "apc.enabled=1",
        "apc.enable_cli=1",
        "pcov.enabled=1"
    ],
    "additional_checks": [
        {
            "name": "Coverage",
            "job": {
                "command": "vendor/bin/phpunit --coverage-clover coverage.xml"
            }
        }
    ]
}
