{
    "extensions": [
        "apcu",
        "pcov"
    ],
    "ini": [
        "apc.enable_cli=1"
    ],
    "additional_checks": [
        {
            "name": "Coverage",
            "job": {
                "command": "vendor/bin/phpunit --coverage-clover coverage.xml"
            }
        }
    ]
}
